{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx":"react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "context/CardContext.jsx", "app/_layout.jsx",  "app/(tabs)/index.jsx", "app/index.jsx"  ]
}
